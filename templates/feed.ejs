<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
	<title type="text"><%= config.siteTitle %></title>
	<link rel="alternate" type="text/html" href="<%= config.siteUrl %>" />
	<link rel="self" type="application/atom+xml" href="<%= config.siteUrl %>/feed" />
	<updated><%= new Date().toISOString() %></updated>
	<author>
		<name><%= config.siteTitle %></name>
	</author>
	<generator version="1.0">Hastily-Written JavaScript</generator>
	<id><%= config.siteUrl %>/feed</id>
<% posts.forEach( post => { %>
	<entry>
		<title type="html"><![CDATA[<%- post.title.rendered %>]]></title>
		<link rel="alternate" type="text/html" href="<%= config.siteUrl %>/<%= post.slug %>" />
		<id><%= config.siteUrl %>/<%= post.slug %></id>
		<published><%= post.date %></published>
		<updated><%= post.modified %></updated>
		<summary type="html"><![CDATA[<%- post.excerpt.rendered %>]]></summary>
		<content type="html"><![CDATA[<%- post.content.rendered %>]]></content>
	</entry>
<% } ); %>
</feed>
